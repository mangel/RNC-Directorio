/*!40000 ALTER TABLE `county` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `curador`
--

DROP TABLE IF EXISTS `curador`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `curador` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(150) DEFAULT NULL,
  `cargo` varchar(150) DEFAULT NULL,
  `email` varchar(150) DEFAULT NULL,
  `telefono` varchar(45) DEFAULT NULL,
  `pagina_web` varchar(150) DEFAULT NULL,
  `subgrupo_taxonomico_id` int(11) NOT NULL,
  `registros_update_id` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_curador_subgrupo_taxonomico1_idx` (`subgrupo_taxonomico_id`),
  KEY `fk_curador_registros_update1_idx` (`registros_update_id`),
  CONSTRAINT `fk_curador_registros_update1` FOREIGN KEY (`registros_update_id`) REFERENCES `registros_update` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_curador_subgrupo_taxonomico1` FOREIGN KEY (`subgrupo_taxonomico_id`) REFERENCES `subgrupo_taxonomico` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
