CREATE TABLE `visitas` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `entidad` varchar(150) DEFAULT NULL,
  `departamento_id` varchar(150) DEFAULT '0',
  `ciudad_id` varchar(150) DEFAULT '0',
  `fecha_visita` datetime DEFAULT NULL,
  `concepto` text,
  `registros_id` int(11) DEFAULT NULL,
  `dilegenciadores_id` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_visitas_dilegenciadores1_idx` (`dilegenciadores_id`),
  CONSTRAINT `fk_visitas_dilegenciadores1` FOREIGN KEY (`dilegenciadores_id`) REFERENCES `dilegenciadores` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
